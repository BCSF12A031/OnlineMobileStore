@model Online_Mobile_Store.Models.Product
@{
    ViewBag.title = "Online Mobile Store";
}

<!DOCTYPE HTML>
<html lang="en" class="no-js">
<head>
    <title>@ViewBag.title</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.js"></script>
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.2.0.min.js"></script>
    <script src="~/Scripts/jquery-2.2.0.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <link href="~/Content/jquery.ui.autocomplete.css" rel="stylesheet" />
    <link href="~/Content/jquery.ui.all.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.theme.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <link href="~/Contents/stylee.css" rel="stylesheet" type="text/css" media="all" />
    <meta name="keywords" content="Online Mobile Store" />
    <link href='//fonts.googleapis.com/css?family=Londrina+Solid|Coda+Caption:800|Open+Sans' rel='stylesheet' type='text/css'>
</head>

<body>
   
    <div class="wrap">
        
        <!----start-Header---->
        <div class="header">
            <div class="search-bar">
               
               
                  
                   @using (Html.BeginForm("GetSearch","Home"))
                   {
                    <input type="text" id="tt1">
                    <input type="submit" value="Search" />
                   }
                
            </div>

            <script>
                $(document).ready(function () {
                   
                    $('#txt').autocomplete({
                        
                            source: '@Url.Action("GetSearch","Home")'
                        });
                });
             
            </script>

            <div class="clear"> </div>
            <div class="header-top-nav">
                <ul>
                    <li>@Html.ActionLink("Register","Register","Users")</li>
                    <li>@Html.ActionLink("Login", "Register", "Users")</li>
                </ul>
            </div>
            <div class="clear"> </div>
        </div>
    </div>
    <div class="clear"> </div>
    <div class="top-header">
        <div class="wrap">
            <!----start-logo---->
            <div class="logo">
                <a href="/Home/Index"><img src="~/Images/logo.png" title="logo" /></a>
            </div>
            <!----end-logo---->
            <!----start-top-nav---->
            <div class="top-nav">
                <ul>
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Store", "Store", "Home")</li>
                    <li>@Html.ActionLink("Featured", "Store", "Home")</li>
                    <li>@Html.ActionLink("Blog", "Blog", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
            </div>
            <div class="clear"> </div>
        </div>
    </div>
    @RenderBody()
    <div class="footer">
        <div class="wrap">
            <div class="section group">
                <div class="col_1_of_4 span_1_of_4">
                    <h3>Our Info</h3>
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur.</p>
                </div>
                <div class="col_1_of_4 span_1_of_4">
                    <h3>Latest-News</h3>
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
                </div>
                <div class="col_1_of_4 span_1_of_4">
                    <h3>Store Location</h3>
                    <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
                    <h3>Order-online</h3>
                    <p>080-1234-56789</p>
                    <p>080-1234-56780</p>
                </div>
                <div class="col_1_of_4 span_1_of_4 footer-lastgrid">
                    <h3>News-Letter</h3>
                    @using (Html.BeginForm())
                    {

                        <input type="text"><input type="submit" value="go" />
                    }
                    <h3>Follow Us:</h3>
                    <ul>
                        <li><a href="https://www.twitter.com"><img src="~/Images/twitter.png" title="twitter" />Twitter</a></li>
                        <li><a href="https://www.facebook.com"><img src="~/Images/facebook.png" title="Facebook" />Facebook</a></li>
                        <li><a href="/Home/Store"><img src="~/Images/rss.png" title="Rss" />Rss</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="clear"> </div>
        <div class="wrap">
            <div class="copy-right">
                <p>&copy; 2016  Online Mobile Store. All Rights Reserved Online Mobile Store</p>
            </div>
        </div>
    </div>




</body>
</html>

